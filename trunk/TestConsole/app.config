<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <sectionGroup name="neptuo.web.framework">
      <section name="components" type="Neptuo.Web.Framework.Configuration.ComponentsSection, Neptuo.Web.Framework.Implementation" />
    </sectionGroup>
  </configSections>

  <neptuo.web.framework>
    <components>
      <controls>
        <add prefix="h" namespace="Neptuo.Web.Framework.Controls" />
      </controls>
      <extensions>
        <add namespace="Neptuo.Web.Framework.Extensions" />
      </extensions>
      <observers>
        <add prefix="ui" name="visible" type="Neptuo.Web.Framework.Observers.VisibleObserver, Neptuo.Web.Framework.Implementation" />
        <add prefix="html" type="Neptuo.Web.Framework.Observers.HtmlObserver, Neptuo.Web.Framework.Implementation" />
        <add prefix="val" type="Neptuo.Web.Framework.Observers.ValidationObserver, Neptuo.Web.Framework.Implementation" />
        <add prefix="cache" type="Neptuo.Web.Framework.Observers.CacheObserver, Neptuo.Web.Framework.Implementation" />
        <add prefix="data" name="DataContext" type="Neptuo.Web.Framework.Observers.DataContextObserver, Neptuo.Web.Framework.Implementation" />
      </observers>
    </components>
  </neptuo.web.framework>
</configuration>